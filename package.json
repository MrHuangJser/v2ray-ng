{
  "name": "v2ray-ng",
  "productName": "V2ray-NG",
  "main": "v2ray-ng/main.js",
  "version": "0.0.0",
  "scripts": {
    "start:renderer": "ng serve --port=4204",
    "start:main": "wait-on http://localhost:4204 && npm run run-main",
    "run-main": "cross-env NODE_ENV=development webpack --config webpack.main.config.js",
    "start:dev": "npm-run-all -p start:renderer start:main",
    "start:app": "./node_modules/.bin/electron --inspect-brk=5858 ./dist/v2ray-ng/main/main.js",
    "lint:style": "stylelint \"src/**/*.less\" --syntax less --fix",
    "lint-staged": "lint-staged",
    "electron-rebuild": "./node_modules/.bin/electron-rebuild",
    "build:mac": "cross-env NODE_ENV=production PLATFORM=darwin webpack --config webpack.main.config.js && ng build && electron-builder -m",
    "build:win": "cross-env NODE_ENV=production PLATFORM=win32 webpack --config webpack.main.config.js && ng build && electron-builder -w"
  },
  "private": true,
  "build": {
    "appId": "com.mrhuangjser.v2ray-ng",
    "mac": {
      "icon": "src/main/assets/dog-colorful.tiff",
      "target": "dmg"
    },
    "dmg": {
      "icon": "src/main/assets/dog-colorful.tiff"
    }
  },
  "dependencies": {
    "7zip-bin": "^5.0.3",
    "@angular/animations": "~9.1.11",
    "@angular/common": "~9.1.11",
    "@angular/compiler": "~9.1.11",
    "@angular/core": "~9.1.11",
    "@angular/forms": "~9.1.11",
    "@angular/platform-browser": "~9.1.11",
    "@angular/platform-browser-dynamic": "~9.1.11",
    "@angular/router": "~9.1.11",
    "@delon/util": "~9.5.3",
    "ajv": "^6.12.3",
    "canvas": "^2.6.1",
    "electron-is": "^3.0.0",
    "electron-log": "^4.2.2",
    "fs": "^0.0.1-security",
    "fs-extra": "^9.0.1",
    "ng-zorro-antd": "^9.3.0",
    "node-7z": "^2.1.1",
    "path": "^0.12.7",
    "request": "^2.88.2",
    "request-progress": "^3.0.0",
    "rxjs": "~6.5.4",
    "sudo-prompt": "^9.2.1",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "electron": "^9.0.3",
    "@angular-devkit/build-angular": "~0.901.8",
    "@angular/cli": "~9.1.8",
    "@angular/compiler-cli": "~9.1.11",
    "@types/ajv": "^1.0.0",
    "@types/fs-extra": "^9.0.1",
    "@types/node": "12",
    "@types/request": "^2.48.5",
    "clean-webpack-plugin": "^3.0.0",
    "codelyzer": "^6.0.0",
    "copy-webpack-plugin": "^6.0.2",
    "cross-env": "^7.0.2",
    "electron-builder": "^22.8.0",
    "electron-rebuild": "^1.11.0",
    "file-loader": "^6.0.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "stylelint": "^13.6.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.3.0",
    "stylelint-order": "^4.1.0",
    "ts-loader": "^7.0.5",
    "ts-node": "~8.3.0",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.8.3",
    "wait-on": "^5.0.1",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  }
}
